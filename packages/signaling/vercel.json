{
  "functions": {
    "api/signal.ts": {
      "runtime": "@vercel/edge@2.0.0"
    },
    "api/session.ts": {
      "runtime": "@vercel/edge@2.0.0"
    },
    "api/health.ts": {
      "runtime": "@vercel/edge@2.0.0"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://vibe-coder.space,https://*.vibe-coder.space,http://localhost:5173,http://localhost:3000"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        },
        {
          "key": "Access-Control-Max-Age",
          "value": "86400"
        }
      ]
    }
  ]
}